[
    {
        "cut": "1",
        "description": "No cuts (but e, ph, and b-tag wgt vetoes)"
    },
    {
        "cut": "MET_filters_fail_bits == 0",
        "description": "MET filter bits pass"
    },
    {
        "cut": "hem_veto",
        "description": "HEM veto",
        "book_plot": "no"
    },
    {
        "cut": "(trig_fired&(1<<3)) == (1<<3)",
        "description": "Trigger fired (120 GeV METNoMu)",
        "book_plot": "no"
    },
    {
        "cut": "MET_pt > 200",
        "description": "MET > 200 GeV",
        "book_plot": "no"
    },
    {
        "cut": "CaloPFMETRatio < 1.0",
        "description": "|CaloMET - PFMET|/CaloMET < 1",
        "book_plot": "no"
    },
    {
        "cut": "reco_PF_n_highPt_jets < 3 && reco_PF_n_highPt_jets > 0",
        "description": "0 < nJets < 3 (pT > 30 GeV)",
        "book_plot": "no",
        "special": "no"
    },
    {
        "cut": "reco_n_bTag_jets == 0",
        "description": "No b-tagged jets",
        "special": "no",
        "book_plot": "no"
    },
    {
        "cut": "abs(reco_PF_jet_corr_eta[0]) < 2.4",
        "description": "leading jet |#eta| < 2.4",
        "book_plot": "no"
    },
    {
        "cut": "reco_PF_jet_corr_pt[0] > 80",
        "description": "Leading jet pT > 80 GeV",
        "book_plot": "no"
    },
    {
        "cut": "MET_jet_dphi0 > 1.5",
        "description": "|#Delta#Phi(MET, leading jet)| > 1.5",
        "book_plot": "no"
    },
    {
        "cut": "All(MET_jet_dphiN > 0.75)",
        "description": "|#Delta#Phi(MET, other jets)| > 0.75",
        "book_plot": "no"
    },

    {
        "cut": "n_good_dsa >= 1",
        "description": "good dSA muons >= 1",
        "book_plot": "no"
    },
    {
        "cut": "n_good_dsa >= 2",
        "description": "good dSA muons >= 2",
        "book_plot": "no"
    },
    {
        "cut": "min_cosalpha > -0.94",
        "description": "Cosmic veto (min(cosalpha) > -0.94)"
    },
    {
        "cut": "best_vtx_dsadsa != 0 && best_dsa_0 != best_dsa_1",
        "description": "Good dSA-dSA vertex",
        "book_plot": "no"
    },
    {
        "cut": "abs(matched_muon_vtx_reduced_chi2) < 4",
        "description": "Vertex #chi^{2} < 4",
        "book_plot": "no"
    },
    {
        "cut": "abs(matched_muon_MT) < 200",
        "description": "MT < 200 GeV",
        "book_plot": "no"
    },
    {
        "cut": "abs(matched_muon_Mmumu) < 30",
        "description": "M_{#mu#mu} < 30 GeV",
        "book_plot": "no"
    },
    {
        "cut": "abs(matched_muon_vtx_dR) < 0.9",
        "description": "dR(muons) < 0.9"
    },
    {
        "cut": "reco_sel_mu_q0 != reco_sel_mu_q1",
        "description": "muons: OSSF"
    },
    {
        "cut": "n_matched == 0",
        "description": "0 GM-dSA matched",
        "inclusive": "no"
    },
    {
        "cut": "n_matched == 1",
        "description": "1 GM-dSA matched",
        "inclusive": "no",
        "print_events": "no"
    },
    {
        "cut": "n_matched == 2",
        "description": "2 GM-dSA matched",
        "inclusive": "no"
    }
]
